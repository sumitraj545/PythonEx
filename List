mylist = ["Orange","Apple","Banana","Cherry","Coffee","Onion","Laptop","Mango"]
print(mylist)
mylist.append("Gava")
print(mylist)
mylist.remove("Orange")
print(mylist)
x=mylist.count("Apple")
print(x)
list1 = [6,4,7,3,9,8]
list1.sort()
print(list1)
list1.reverse()
print(list1)
mylist1 = mylist.copy()
print(mylist1)
mylist.sort()
print(mylist)
mylist.reverse()
print(mylist)
mylist.insert(2,"Shop")
print(mylist)
mylist.pop()
print(mylist)
del mylist[2]
print(mylist)
mylist.clear()
print(mylist)
